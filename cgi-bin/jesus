#!/usr/bin/bash

cat  << EOF
                                 |~~~~~~~|
                                 |       |
                                 |       |
                                 |       |
                                 |       |
                                 |       |
      |~.\\\\\\_\\~~~~~~~~~~~~~~xx~~~         ~~~~~~~~~~~~~~~~~~~~~/_//;~|
      |  \\  o \\_         ,XXXXX),                         _..-~ o /  |
      |    ~~\\  ~-.     XXXXX\`)))),                 _.--~~   .-~~~   |
       ~~~~~~~\`\\   ~\\~~~XXX' _/ ';))     |~~~~~~..-~     _.-~ ~~~~~~~
                \`\\   ~~--\`_\\~\\, ;;;\\)__.---.~~~      _.-~
                  ~-.       \`:;;/;; \\          _..-~~
                     ~-._      \`''        /-~-~
                         \`\\              /  /
                           |         ,   | |
                            |  '        /  |
                             \\/;          |
                              ;;          |
                              \`;   .       |
                              |~~~-----.....|
                             | \\             \\
                            | /\\~~--...__    |
                            (|  \`\\       __-\\|
                            ||    \\_   /~    |
                            |)     \\~-'      |
                             |      | \\      '
                             |      |  \\    :
                              \\     |  |    |
                               |    )  (    )
                                \\  /;  /\\  |
                                |    |/   |
                                |    |   |
                                 \\  .'  ||
                                 |  |  | |
                                 (  | |  |
                                 |   \\ \\ |
                                 || o \`.)|
                                 |\`\\\\) |
                                 |       |
                                 |       |
EOF

read msg
echo $msg
